{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "test": "jest --runInBand",
    "pretest": "yarn",
    "build": "yarn babel . -d dist --ignore 'node_modules/**/*.js' --source-maps",
    "serve": "yarn && yarn build && node dist/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.5.11",
    "sinon": "^7.3.2",
    "sinon-mongoose": "^2.3.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  }
}
